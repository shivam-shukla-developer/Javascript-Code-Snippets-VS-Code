{
  "name": "js-code-snippets",
  "displayName": "JavaScript Code Snippets",
  "publisher": "ShivamShukla",
  "description": "This extension contains javascript code snippets for Visual Studio Code editor.",
  "version": "0.1.0",
  "repository": "https://github.com/shivam-shukla-developer/Javascript-Code-Snippets-VS-Code",
  "engines": {
    "vscode": "^1.74.0"
  },
  "icon": "images/icon.png",
  "categories": [
    "Snippets"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [],
    "snippets": [
      {
        "language": "javascript",
        "path": "./src/snippets/snippets.json"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}
